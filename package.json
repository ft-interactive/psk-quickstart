    "stage": 0,
    "blacklist": [
      "regenerator"
    ]
  },
  "private": true,
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babelify": "^6.4.0",
    "bluebird": "^3.0.2",
    "browserify": "^12.0.1",
    "chalk": "^1.1.1",
    "eslint": "^1.7.3",
    "eslint-config-exhibit": "^2.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "inquirer": "^0.11.0",
    "loud-rejection": "^1.0.0",
    "nodemon": "^1.8.0",
    "sander": "^0.4.0",
    "source-map-support": "^0.3.3",
    "tildify": "^1.1.2"
  },
  "eslintConfig": {
    "extends": "exhibit"
  },
  "scripts": {
    "build": "browserify --bare --entry quickstart.js -t babelify > quickstart.bundled.js",
